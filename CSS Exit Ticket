		<script src="external.js"></script> <!-- external javascript -->
		<!-- internal style -->
		<style>
			h2 {
				color: #696969;
				text-align: center;
			}
		</style>
		<!-- internal javascript -->
		<script>
			function internal(){
				document.getElementById("normal").innerHTML = "also internally stupid";
			}
		
		
		</script>
		

	</head>
	<body>
		<h1 style = "color:blue; font-size: 2vw; border: 2px solid red"> Hello, what are you doing? </h1> <!-- in-line style -->
		<h1> Externally stupid </h1>
		<h2 id = "normal"> Internally Smart</h2>
		<button type = "button" onmouseup="myExtFunc()" 
			onmousedown="internal()">Don't press this button</button> 
	</body>

